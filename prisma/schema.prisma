generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
 
model Goods {
  id Int @id @default(autoincrement())
  slug String @unique
  img String
  mobileImg String
  title String
  price String
  category String
  descr String
  vendorCode String
  brend String
  application String
  howWork String
  foolDescr String
}